<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yue.infrastructure.dao.IVoucherOrderDao">

    <resultMap id="CaseMap" type="com.yue.infrastructure.dao.po.VoucherOrder">
        <id column="id" property="id"/>
        <result column="user_id" property="userId"/>
        <result column="voucher_id" property="voucherId"/>
        <result column="pay_type" property="payType"/>
        <result column="status" property="status"/>
        <result column="create_time" property="createTime"/>
        <result column="pay_time" property="payTime"/>
        <result column="use_time" property="useTime"/>
        <result column="refund_time" property="refundTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>

    <insert id="addVoucherOrder" parameterType="com.yue.infrastructure.dao.po.VoucherOrder">
        insert into tb_voucher_order(user_id,voucher_id,order_id,status)
        values(#{userId},
               #{voucherId},
               #{orderId},
               #{status})
    </insert>


</mapper>